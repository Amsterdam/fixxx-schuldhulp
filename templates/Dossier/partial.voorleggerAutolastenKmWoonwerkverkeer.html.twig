{% import 'partial.macros.html.twig' as tools %}
            <div class="accordion {{ app.request.isXmlHttpRequest ? 'active' }}" id="voorlegger-autolastenKmWoonwerkverkeer">
                <div class="accordion-header">
                    
                    <div class="voorlegger-header">
                        <h3 class="naam">
                            <a href="#voorlegger-autolastenKmWoonwerkverkeer" data-handler="toggle">Autolasten km woon-werk-verkeer</a>
                        </h3>
                        <div class="status-container">
                          {{ form_row(form.autolastenKmWoonwerkverkeerOntvangenMadi) }}
