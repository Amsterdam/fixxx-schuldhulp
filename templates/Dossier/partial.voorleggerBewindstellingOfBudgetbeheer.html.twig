{% import 'partial.macros.html.twig' as tools %}
            <div class="accordion {{ app.request.isXmlHttpRequest ? 'active' }}" id="voorlegger-bewindstellingOfBudgetbeheer">
                <div class="accordion-header">
                    
                    <div class="voorlegger-header">
                        <h3 class="naam">
                            <a href="#voorlegger-bewindstellingOfBudgetbeheer" data-handler="toggle">Onderbewindstelling of Budgetbeheer</a>
                        </h3>
                        <div class="nvt">
                            <span class="{{ dossier.voorlegger.bewindstellingOfBudgetbeheerNvt ? 'nvt' : 'open' }}">{{ dossier.voorlegger.bewindstellingOfBudgetbeheerNvt ? 'n.v.t.' }}</span>
                        </div>
                    </div>
                    
                </div>
                <div class="accordion-body">
                    <div class="voorlegger-body">
                        {{ form_row(form.bewindstellingOfBudgetbeheerNvt) }}
