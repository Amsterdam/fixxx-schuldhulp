{% import 'partial.macros.html.twig' as tools %}
            <div class="accordion {{ app.request.isXmlHttpRequest ? 'active' }}" id="voorlegger-budgetbeheer">
                <div class="accordion-header">
                    <a href="#voorlegger-budgetbeheer" data-handler="toggle">
                        <div class="voorlegger-header">
                            <div class="naam">
                                Budgetbeheer
                            </div>
                            <div class="status">
                            </div>
                            <div class="nvt">
                                <span class="{{ dossier.voorlegger.budgetbeheerNvt ? 'nvt' : 'open' }}">{{ dossier.voorlegger.budgetbeheerNvt ? 'n.v.t.' }}</span>
                            </div>
                        </div>
                    </a>
                </div>
                <div class="accordion-body">
                    <div class="voorlegger-body">
                        {{ form_row(form.budgetbeheerNvt) }}
                        {{ form_row(form.budgetbeheerPlangroep) }}
                        {{ form_row(form.budgetbeheerCav) }}
                        {{ form_row(form.budgetbeheerFibu) }}
                        {{ form_row(form.budgetbeheerOverig) }}
                        {% include 'partial.files-container.html.twig' with {'form_elm': form.file, 'defaultDocumentNaam': 'Budgetbeheer'} only %}
                        {% include 'partial.files-list.html.twig' with {'dossierDocumenten': dossier.getDocumentenByOnderwerp('budgetbeheer')} only %}
                    </div>
                </div>
            </div>
