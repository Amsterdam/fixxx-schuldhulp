{% import 'partial.macros.html.twig' as tools %}
            <div class="accordion {{ app.request.isXmlHttpRequest ? 'active' }}" id="voorlegger-vrijwaringsbewijs">
                <div class="accordion-header">
                    
                        <div class="voorlegger-header">
                            <h3 class="naam">
                                <a href="#voorlegger-vrijwaringsbewijs" data-handler="toggle">Vrijwaringsbewijs</a>
                            </h3>
                            <div class="status-container">
                              {{ form_row(form.vrijwaringsbewijsOntvangenMadi) }}
                              {{ form_row(form.vrijwaringsbewijsOntvangenGka) }}
                            </div>
                            <div class="nvt">
                                <span class="{{ dossier.voorlegger.vrijwaringsbewijsNvt ? 'nvt' : 'open' }}">{{ dossier.voorlegger.vrijwaringsbewijsNvt ? 'n.v.t.' }}</span>
                            </div>
                        </div>
                    
                </div>
                <div class="accordion-body">
                    <div class="voorlegger-body">
                        {{ form_row(form.vrijwaringsbewijsNvt) }}
                        <div class="document-bestanden">
                            {% include 'partial.files-container.html.twig' with {'form_elm': form.file, 'defaultDocumentNaam': 'Vrijwaringsbewijs'} only %}
                            {% include 'partial.files-list.html.twig' with {'dossierDocumenten': dossier.getDocumentenByOnderwerp('vrijwaringsbewijs')} only %}
                        </div>
                    </div>
                </div>
            </div>
