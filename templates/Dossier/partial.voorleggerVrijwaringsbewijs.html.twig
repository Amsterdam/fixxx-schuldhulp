
            {{ form_start(form, {'attr': {'class': 'ajax default-form-style'} }) }}
            <div class="accordion" id="voorlegger-vrijwaringsbewijs">
                <div class="accordion-header">
                    <div class="voorlegger-header">
                        <div class="naam">
                            <a href="#voorlegger-vrijwaringsbewijs">Vrijwaringsbewijs</a>
                        </div>
                        <div class="status">
                            <span class="{{ dossier.voorlegger.vrijwaringsbewijsOntvangenMadi ? 'afgerond' : 'open' }}">MaDi</span>
                            <span class="{{ dossier.voorlegger.vrijwaringsbewijsOntvangenGka ? 'afgerond' : 'open' }}">GKA</span>
                        </div>
                        <div class="nvt">
                            <span class="{{ dossier.voorlegger.vrijwaringsbewijsNvt ? 'nvt' : 'open' }}">{{ dossier.voorlegger.vrijwaringsbewijsNvt ? 'n.v.t.' }}</span>
                        </div>
                    </div>
                </div>
                <div class="accordion-body">
                    <div class="voorlegger-body">
                        {{ form_row(form.vrijwaringsbewijsNvt) }}
                        {{ form_row(form.vrijwaringsbewijsOntvangenMadi) }}
                        {{ form_row(form.vrijwaringsbewijsOntvangenGka) }}
                        {% include 'partial.files-container.html.twig' with {'form_elm': form.file} only %}
                        {% include 'partial.files-list.html.twig' with {'dossierDocumenten': dossier.getDocumentenByOnderwerp('vrijwaringsbewijs')} only %}
                        <div class="nav">
                            <button type="submit">Opslaan</button>
                        </div>
                    </div>
                </div>
            </div>
            {{ form_end(form) }}